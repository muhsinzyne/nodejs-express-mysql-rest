{
  "scripts": {
    "start:local": "NODE_ENV=local ts-node src/server.ts",
    "start:dev": "NODE_ENV=development ts-node src/server.ts",
    "start:staging": "NODE_ENV=staging ts-node src/server.ts",
    "start:prod": "NODE_ENV=production ts-node src/server.ts",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "sequelize": "ts-node sequelize.ts",
    "make:migration": "node scripts/generate_migration.js",
    "migrate": "npm run sequelize db:migrate",
    "migrate:undo": "npm run sequelize db:migrate:undo",
    "migrate:undo:all": "npm run sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "mysql2": "^3.11.3",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "typescript-eslint-parser": "^21.0.2"
  }
